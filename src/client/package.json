{
    "name": "gherkin-client",
    "displayName": "gherkin-language-support",
    "description": "",
    "version": "1.0.0-semantically-released",
    "publisher": "iteratec-gmbh",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "vscode": "^1.1.21",
        "vscode-languageclient": "^5.0.1"
    },
    "devDependencies": {
        "@types/node": "^8.10.28",
        "@types/mocha": "^5.2.5",
        "typescript": "^3.0.1",
        "tslint": "^5.11.0"
    }
}
